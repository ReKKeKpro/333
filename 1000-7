using System;
using System.Collections.Generic;
using System.Diagnostics.CodeAnalysis;
using System.Linq;
using System.Text;
using System.Threading;
using System.Threading.Tasks;

namespace ConsoleApp4
{
    internal class Program
    { }
    
    

namespace ConsoleApp4 
        
    {
        internal class Program
        {

            static void Main(string[] args)
            {
                int a = 1000;

                while (a > 0)
                {
                    a--;
                    a--;
                    a--;
                    a--;
                    a--;
                    a--;
                    a--;
                    

                    Console.WriteLine(a);
                }


                }



            }
        }
    }



